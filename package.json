{
  "name": "tulip",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:web": "next dev --turbopack",
    "dev:server": "tsx watch server/server.ts",
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:server\"",
    "build": "next build --turbopack",
    "start:web": "next start",
    "start:server": "node server/server.ts",
    "start": "concurrently \"npm run start:web\" \"npm run start:server\"",
    "lint": "eslint . --ext .js,.ts,.tsx"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "konva": "^10.0.2",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "react-konva": "^19.0.10",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
